
// function不需要返回值
function sum(num1, num2) {
    return num1 + num2;
}

function sayHi() {
    console.log('sayhi1' + arguments[0] + ' ' + arguments[1]);
}

// 函数同名，最后的函数会覆盖前面同名的函数z
function sayHi() {
    console.log('sayhi2' + arguments[0] + ' ' + arguments[1]);
}

console.log(sum(1, 2));
sayHi('hello', 'world');
